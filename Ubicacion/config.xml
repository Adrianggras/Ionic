<?xml version="1.0" encoding="UTF-8" ?>
<widget xmlns   = "http://www.w3.org/ns/widgets"
    xmlns:gap   = "http://phonegap.com/ns/1.0"
    id          = "com.ubicaciones.adrigimenez"
    versionCode = "1"
    version     = "0.0.1" >
	<icon src="icono.jpg" width="57" height="57" density="mdpi" />
    
  <name>Where's my car?</name>
  <description>
      Guarda tu ubicación junto con una imagen para recordar dónde has estado
  </description>
  <author href="https://www.linkedin.com/in/adrian-gimenez-dev" email="adrianggras@gmail.com">
      Adrián Giménez Gras
  </author>

  <access origin="*" />
  <allow-navigation href="*" />
  <allow-intent href="*" />

  <plugin name="cordova-plugin-whitelist" />
  <plugin name="cordova-plugin-vibration" />
  <plugin name="cordova-plugin-x-socialsharing" spec="5.4.0" />
  <plugin name="cordova-plugin-geolocation" />
  <plugin name="cordova-plugin-camera" />

  <preference name="phonegap-version" value="cli-9.0.0" />

</widget>